{
    "name": "aurorum/flarum-panorama",
    "description": "A Flarum extension.This extension makes it possible to add a panorama picture to your post.",
    "keywords": [
         "panorama",
         "flarum",
         "aurorum"
    ],
    "type": "flarum-extension",
    "support": {
        "issues": "https://github.com/Aurorum-Studio/flarum-better-iframe/issues",
        "source": "https://github.com/Aurorum-Studio/flarum-better-iframe",
        "forum": "https://discuss.flarum.org",
        "website": "https://dev.aurorum.co"
    },
    "homepage": "https://dev.aurorum.co",
    "require": {
        "flarum/core": "^1.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Aurorum-Studio"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Aurorum-Studio\\panorama\\": "src/"
        }
    },
    "extra": {
        "flarum-extension": {
            "title": "Panorama",
            "icon": {
                "name": "fab fa-phoenix-framework",
                "backgroundColor": "#238c59",
                "color": "#000"
            }
        }
    }
}
